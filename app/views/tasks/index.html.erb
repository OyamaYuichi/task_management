<p id="notice"><%= notice %></p>

<h1>タスク一覧</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Detail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= task.name %></td>
        <td><%= task.detail %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>


